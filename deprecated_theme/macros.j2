
{% macro render_basic_menu() -%}
<img src="/media/logo/linkedart-logo-horizontal.png" alt="Linked Art logo" class="logo">
<ul id="main_nav_list">
{% for nav_item in nav %}
    <li><a {% if nav_item.active %}class="selected" {% endif -%}
        href="{{ nav_item.url|url }}">{{ nav_item.title }}</a></li>
{% endfor %}
</ul>
{%- endmacro %}
